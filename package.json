{
  "name": "ai-brainstorm",
  "version": "1.0.0",
  "description": "Multi-AI discussion platform with role-based conversation and knowledge management",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm:dev:backend\" \"vite\"",
    "dev:backend": "nodemon --watch src --ext ts,js,json --exec \"tsc && node dist/index.js\"",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "test": "jest"
  },
  "keywords": [
    "ai",
    "discussion",
    "multi-model",
    "knowledge-base"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@element-plus/icons-vue": "^2.3.2",
    "@google/generative-ai": "^0.24.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "element-plus": "^2.10.6",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "marked": "^16.1.2",
    "moment": "^2.30.1",
    "openai": "^5.12.2",
    "uuid": "^11.1.0",
    "vite": "^7.1.2",
    "vue": "^3.5.18",
    "ws": "^8.18.3"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node",
    "transpileOnly": true,
    "swc": false,
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "NodeNext"
    }
  },
  "devDependencies": {
    "@railway/cli": "^4.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.10",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}